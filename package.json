{
  "name": "@form8ion/eslint-config-jest",
  "description": "shareable config for ESLint for Jest test files",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "repository": "form8ion/eslint-config-jest",
  "bugs": "https://github.com/form8ion/eslint-config-jest/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-jest",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.14",
    "@form8ion/eslint-config": "1.7.1",
    "@form8ion/remark-lint-preset": "1.0.11",
    "@travi/travis-lint": "1.0.1",
    "ban-sensitive-files": "1.9.12",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.0",
    "lockfile-lint": "4.3.7",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0",
    "remark-toc": "7.0.0",
    "remark-usage": "9.0.0"
  },
  "dependencies": {
    "eslint-plugin-jest": "24.1.3",
    "typescript": "^4.0.0"
  }
}
